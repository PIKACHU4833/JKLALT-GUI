    end
    for i = 0,255,10 do
        wait()
        script.Parent.TextColor3 = Color3.new(i/255,0/255,255/255)
    end
    for i = 255,0,-10 do
        wait()
        script.Parent.TextColor3 = Color3.new(255/255,0/255,i/255)
