        wait(0.0)
        script.Parent.TextColor3 = Color3.new(0/255,i/255,255/255)
    end
    for i = 0,255,10 do
        wait(0.0)
        script.Parent.TextColor3 = Color3.new(i/255,0/255,255/255)
    end
    for i = 255,0,-10 do
        wait(0.0)
        script.Parent.TextColor3 = Color3.new(255/255,0/255,i/255)
    end
end
end
coroutine.wrap(PKIPWI_fake_script)()
