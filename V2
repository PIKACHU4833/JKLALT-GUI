DISCO.Parent = SCROLLscript
DISCO.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
DISCO.BorderColor3 = Color3.fromRGB(196, 40, 28)
DISCO.BorderSizePixel = 3
