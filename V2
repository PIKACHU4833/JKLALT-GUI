DECAL.Parent = SCROLLscript
DECAL.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
DECAL.BorderColor3 = Color3.fromRGB(196, 40, 28)
DECAL.BorderSizePixel = 3
